#if get(g:, '@optionprefix_transp_bg', 0) && !has('gui_running')
Variant: 256 8
#if get(g:, '@optionprefix_transp_bg', 0)
Variant: gui 256 8
    Normal           white             none
    NormalNC         brightgrey        none
    Terminal         white             none
#else
    Normal           white             black
    NormalNC         brightgrey        brightblack
    Terminal         white             black
#endif

